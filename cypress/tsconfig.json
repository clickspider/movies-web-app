{
  "extends": "../tsconfig.json",
  "include": [
    "../node_modules/cypress",
    "./**/*.ts"
  ],
  "exclude": [
    "../node_modules"
  ],
  "compilerOptions": {
    "lib": [
      "es2015",
      "dom"
    ],
    "types": [
      "cypress",
      "cypress-axe",
      "cypress-firebase"
    ],
    "isolatedModules": false,
    "allowJs": true,
    "noEmit": true
  }
}